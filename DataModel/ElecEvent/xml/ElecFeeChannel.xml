                <code>
                    return m_adc;
                </code>
            </method>

            <method
                name="tdc"
                type="std::vector&lt;unsigned int&gt;&amp; "
                desc=""
                />

            <method
                name="tdc"
                type="const std::vector&lt;unsigned int&gt;&amp; "
                desc=""
                const="TRUE"
                />   
        </class>
    </package>
